            </basicHttpBinding>
        </bindings>
        <client>
            <endpoint address="https://serviciosdemo.reconoser.com.co:6300/Service.svc" 
                      binding="basicHttpBinding" 
                      bindingConfiguration="BasicHttpsBinding_IService" 
                      contract="ServicioPrueba.IService" 
                      name="BasicHttpsBinding_IService"/>
        </client>
