        </service>
      </services-->
      <behaviors>
        <serviceBehaviors>
          <behavior name="BasicHttpsBinding_IService" >
            <!-- Add the following element to your service behavior configuration. -->
            <serviceMetadata httpGetEnabled="true" />
            <serviceDebug includeExceptionDetailInFaults="False" />
          </behavior>
        </serviceBehaviors>
